<Window x:Class="mysqltesting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mysqltesting"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="label" Content="Name" HorizontalAlignment="Left" Height="35" Margin="90,85,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBoxName" HorizontalAlignment="Left" Height="35" Margin="190,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="215"/>
        <Label x:Name="label1" Content="Password" HorizontalAlignment="Left" Height="35" Margin="90,160,0,0" VerticalAlignment="Top" Width="65"/>
        <Button x:Name="button" Content="Log in" HorizontalAlignment="Left" Height="35" Margin="155,235,0,0" VerticalAlignment="Top" Width="85" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="35" Margin="190,160,0,0" VerticalAlignment="Top" Width="215"/>

    </Grid>
</Window>
